{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "airbnb",
        "react-app",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "prettier",
        "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-one-expression-per-line": ["off"],
      "react/require-default-props": ["off"],
      "react/forbid-prop-types": ["off"]
    }
}
